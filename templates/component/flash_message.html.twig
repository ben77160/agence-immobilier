<div class="mt-5">
    {% for flashMessage in app.session.flashBag.get('success') %}
        <div class="message-success" role="alert">
            {{ flashMessage }}
        </div>
    {% endfor %}
    {% for flashMessage in app.session.flashBag.get('error') %}
        <div class="message-danger" role="alert">
            {{ flashMessage }}
        </div>
    {% endfor %}
</div>